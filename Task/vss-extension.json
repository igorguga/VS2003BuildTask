{
  "manifestVersion": 1,
  "id": "VS2003-Build-Task",
  "version": "0.1.1",
  "name": "Visual Studio 2003 Build Task",
  "scopes": [ "vso.build_execute", "vso.build" ],
  "description": "Builds Visual Studio .NET 2003 solutions and project files.",
  "baseUri": "",
  "publisher": "igorrosamacedo",
  "public": true,
  "icons": {
    "default": "img/logo.png"
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "tags": [
    "Build",
    "VS2003",
    ".net 1.1"
  ],
  "screenshots": [
        {
            "path": "screenshots/AddTask.png"
        },
        {
            "path": "screenshots/ConfigTask.png"
        },
        {
            "path": "screenshots/BuildLog.png"
        }
    ],
  "links": {
    "home": {
      "uri": "https://github.com/igorguga/VS2003BuildTask"
    },
    "getstarted": {
      "uri": "https://github.com/igorguga/VS2003BuildTask/blob/master/README.md"
    },
    "support": {
      "uri": "https://github.com/igorguga/VS2003BuildTask/issues/new"
    },
    "repository": {
      "uri": "https://github.com/igorguga/VS2003BuildTask"
    },
    "issues": {
      "uri": "https://github.com/igorguga/VS2003BuildTask/issues"
    }
  },
  "branding": {
    "color": "rgb(220, 235, 252)",
    "theme": "light"
  },
  "content":
  {
    "details": {
      "path": "details.md"
    },
    "license": {
      "path": "license-terms.txt"
    }
  },
  "files": [
    {
      "path": "BuildVS2003"
    }
  ],
  "categories": [
    "Build and release"
  ],
  "contributions": [
    {
      "id": "buildvs2003",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "type": "ms.vss-distributed-task.task",
      "properties": {
        "name": "BuildVS2003"
      }
    }
  ]
}
